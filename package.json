    "Music notation",
    "addressing",
    "selection",
    "highlight"
  ],
  "author": "Raff Viglianti, University of Maryland",
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-cli": "6.2.4",
    "babel-preset-es2015": "6.6.0",
    "babel-register": "6.2.0",
    "babelify": "7.2.0",
    "browserify": "14.1.0",
    "gulp": "3.9.1",
    "gulp-autoprefixer": "3.1.0",
    "gulp-connect": "3.1.0",
    "gulp-cssnano": "2.1.1",
    "gulp-rename": "1.2.2",
    "gulp-sass": "2.2.0",
    "gulp-sourcemaps": "1.6.0",
    "gulp-uglify": "1.5.3",
    "gulp-util": "3.0.7",
    "vinyl-buffer": "1.0.0",
    "vinyl-source-stream": "1.1.0"
  },
  "dependencies": {
    "backbone": "1.3.1",
