  "main": "./lib/js/index.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublish": "npm run build"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
